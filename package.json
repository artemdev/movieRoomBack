{
    "name": "template",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "cross-env NODE_ENV=production node ./bin/server.js",
        "start:dev": "cross-env NODE_ENV=development nodemon ./bin/server.js",
        "lint": "eslint \"**/*.js\"",
        "lint:fix": "eslint --fix \"**/*.js\"",
